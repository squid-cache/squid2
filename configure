  { $as_echo "$as_me:${as_lineno-$LINENO}: result: $ac_krb5_config" >&5
$as_echo "$ac_krb5_config" >&6; }
else
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
